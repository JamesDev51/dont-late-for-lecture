<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".view.mainview.MyPageFragment" android:id="@+id/frameLayout6">
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="parent" android:layout_margin="30dp" android:id="@+id/linearLayout2" tools:layout_editor_absoluteX="30dp"
            app:layout_constraintBottom_toTopOf="@+id/btn_logout">
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                style="@style/loginHeader"
                android:text="내 정보"
                android:id="@+id/textView14"
        />
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="50dp"
        >
            <TextView
                    android:text="이름 : "
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/head_name" android:textAppearance="@style/TextAppearance.AppCompat.Medium" tools:ignore="HardcodedText"/>
            <TextView
                    android:text="정민석"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/txt_name" android:layout_weight="1" android:textAppearance="@style/TextAppearance.AppCompat.Medium" tools:ignore="HardcodedText"/>
        </LinearLayout>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="50dp">
            <TextView
                    android:text="이메일 : "
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/head_email" android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
            <TextView
                    android:text="asdf@gmail.com"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/txt_email" android:layout_weight="1" android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
        </LinearLayout>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="50dp">
            <TextView
                    android:text="휴대폰 : "
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/head_phone" android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
            <TextView
                    android:text="010-1234-1234"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/txt_phone" android:layout_weight="1" android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
        </LinearLayout>
    </LinearLayout>
    <Button
            android:text="로그아웃하기"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/btn_logout" app:backgroundTint="#1B32B3" tools:layout_editor_absoluteX="0dp" app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="4dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>